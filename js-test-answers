6. 	A) 	const names = people.map(person => person.name);
		console.log(names);

	B) 	const peopleWithSalary = people.map(person => ({
  		...person,
  		salary: 50000
		});
		console.log(peopleWithSalary);

	C) 	const peopleAbove30 = people
  		.filter(person => person.age > 30)
  		.map(person => ({
    		name: person.name,
    		age: person.age
  		});
		console.log(peopleAbove30);

7.	 function main(cb1, cb2, x, y) {
    	console.log("Add : ", cb1(x, y));
    	console.log("Subtract : ", cb2(x, y));
  	}
  	const cb1 = (a, b) => a + b;
  	const cb2 = (a, b) => a - b;
  	const x = 11;
  	const y = 4;
  	main(cb1, cb2, x, y);

8.	var users = [
    	{firstName: "Susan", lastName: "Steward"},
    	{firstName: "Daniel", lastName: "Longbottom"},
    	{firstName: "Jacob", lastName: "Black"}
	  ];
	  var out = users.map(user => `${user.firstName} ${user.lastName}`);
	  console.log(out);

